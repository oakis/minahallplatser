{
  "name": "minahallplatser",
  "version": "0.9.0",
  "versionCode": "64",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "test": "jest --forceExit",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage",
    "clean": "cd android && ./gradlew clean",
    "build-debug": "curl http://localhost:8081/index.android.bundle?platform=android -o android/app/src/main/assets/index.android.bundle && cd android && gradlew clean && gradlew assembleDebug",
    "build-release": "yarn clean && cd android && gradlew assembleRelease",
    "debug": "yarn clean && react-native run-android --appIdSuffix dev",
    "release": "yarn clean && cd android && gradlew installRelease",
    "adb": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:5000 tcp:5000"
  },
  "dependencies": {
    "base-64": "0.1.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "lodash": "4.17.4",
    "moment": "2.21.0",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-cancelable-fetch": "0.1.1",
    "react-native-device-info": "0.21.5",
    "react-native-fabric": "0.5.1",
    "react-native-fbsdk": "0.8.0",
    "react-native-firebase": "4.0.5",
    "react-native-geolocation-service": "1.0.4",
    "react-native-mixpanel": "0.0.16",
    "react-native-router-flux": "4.0.0-beta.28",
    "react-native-vector-icons": "4.3.0",
    "react-redux": "5.1.1",
    "redux": "3.6.0",
    "redux-mock-store": "1.4.0",
    "redux-thunk": "2.1.0",
    "sinon": "4.5.0"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "17.0.2",
    "eslint-config-rallycoding": "3.1.0",
    "jest": "22.4.3",
    "metro-react-native-babel-preset": "0.45.0",
    "react-dom": "16.6.3",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>src/setupTests.js"
    ],
    "globals": {
      "window": true
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  }
}
