{
  "name": "minahallplatser",
  "version": "0.8.0",
  "versionCode": "63",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage",
    "clean": "cd android && gradlew clean",
    "build-debug": "curl http://localhost:8081/index.android.bundle?platform=android -o android/app/src/main/assets/index.android.bundle && cd android && gradlew clean && gradlew assembleDebug",
    "build-release": "npm run clean && cd android && ./gradlew assembleRelease",
    "debug": "npm run clean && react-native run-android --appIdSuffix dev"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.21.0",
    "react": "^16.3.1",
    "react-native": "^0.55.3",
    "react-native-cancelable-fetch": "^0.1.1",
    "react-native-device-info": "^0.21.5",
    "react-native-fabric": "^0.5.1",
    "react-native-fbsdk": "^0.7.0",
    "react-native-firebase": "^4.0.5",
    "react-native-geolocation-service": "^1.0.4",
    "react-native-mixpanel": "0.0.16",
    "react-native-router-flux": "^4.0.0-beta.28",
    "react-native-vector-icons": "^4.3.0",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-mock-store": "1.4.0",
    "redux-thunk": "^2.1.0",
    "sinon": "^4.5.0"
  },
  "devDependencies": {
    "babel-jest": "17.0.2",
    "babel-preset-react-native": "1.9.0",
    "eslint-config-rallycoding": "^3.1.0",
    "jest": "^22.4.3",
    "react-dom": "^16.3.2",
    "react-test-renderer": "15.4.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>src/setupTests.js"
    ],
    "globals": {
      "window": true
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}"
    ]
  }
}
